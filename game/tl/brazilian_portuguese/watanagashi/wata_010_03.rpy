translate brazilian_portuguese wata_010_03_652a370a:

    # nvl clear
    # n "It was because tonight...{w=1.0} someone had already disappeared."
    nvl clear
    n "Porque àquela noite...{w=1.0} alguém já tinha desaparecido."

# game/watanagashi/wata_010_03.rpy:1413
translate brazilian_portuguese wata_010_03_67337d64:

