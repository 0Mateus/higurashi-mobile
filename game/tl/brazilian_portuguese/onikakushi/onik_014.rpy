# game/onikakushi/onik_014.rpy:396
translate brazilian_portuguese onik_014_251ba127:

    # nvl clear
    # n "...In the worst case... my own house might not be safe anymore..."
    nvl clear
    n "...No pior caso... minha própria casa poderia não mais ser segura..."

# game/onikakushi/onik_014.rpy:399
translate brazilian_portuguese onik_014_3ead27fb:

    # n "That was an incredibly frightening thought..."
    n "Isso era um pensamento assustador..."

