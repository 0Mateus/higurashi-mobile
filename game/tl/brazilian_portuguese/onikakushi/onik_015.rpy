# game/onikakushi/onik_015.rpy:86
translate brazilian_portuguese onik_015_1c8686bc:

    # nvl clear
    # n "Take note of the plates of any car that drives by."
    nvl clear
    n "Tome nota das placas de qualquer carro que esteja na estrada."

# game/onikakushi/onik_015.rpy:89
